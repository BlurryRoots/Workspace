source $WS_CFG_PATH/ws-shared
source $WS_MY_PLUGIN_ROOT/shared

# copy
_ws_log verbose "Linking $subl_keymap_name to $subl_config_path/$subl_keymap_name"

LN=/bin/ln
LN_FLAGS=-s
if _ws_should_log $_WS_CFG_LOG_VERBOSE; then
	LN_FLAGS+=v
fi

_ws_log verbose $($LN $LN_FLAGS "$WS_MY_PLUGIN_ROOT/$subl_keymap_name" "$subl_config_path/$subl_keymap_name")
